config_version=5

[application]

config/name="Invasion Defense: Sector Null"
config/description="Invasion Defense!"
config/tags=PackedStringArray("concepts", "fullgame")
run/main_scene="res://TSCN Scenes/Main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://sprites/Newflashlogo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://sprites/Profile1.png"

[autoload]

GlobalScript="*res://scripts/GlobalScript.gd"
Transition="*res://TSCN Scenes/transition.tscn"
GlobalSql="*res://scripts/GlobalSQL.gd"
ClSettings="*res://TSCN Scenes/cl_settings.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-sqlite/plugin.cfg")

[file_customization]

folder_colors={
"res://TSCN Scenes/": "green",
"res://scripts/": "purple",
"res://sprites/": "red"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

